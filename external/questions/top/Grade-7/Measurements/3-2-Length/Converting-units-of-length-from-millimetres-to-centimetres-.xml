    <questionvariables>
      <text>mm_1:rand(89)+10;/*First length in millimetres*/
mm_2:rand(400)+100;/*Second length in millimetres*/
mm_3:rand(499)+501;/*Third length in millimetres*/
cm_1:decimalplaces(mm_1/10,1);/*First length in centimetres*/
cm_2:decimalplaces(mm_2/10,1);/*Second length in centimetres*/
cm_3:decimalplaces(mm_3/10,1);/*Third length in centimetres*/

/*Mistake_1*/
cm_1_mult:mm_1*10;/*First multiplied length in centimetres*/
cm_2_mult:mm_2*10;/*Second multiplied length in centimetres*/
cm_3_mult:mm_3*10;/*Third multiplied length in centimetres*/

/*Mistake_2*/
cm_1_add:mm_1+10;/*First added length in centimetres*/
cm_2_add:mm_2+10;/*Second added length in centimetres*/
cm_3_add:mm_3+10;/*Third added length in centimetres*/

/*Mistake_3*/
cm_1_sub:mm_1-10;/*First subtracted length in centimetres*/
cm_2_sub:mm_2-10;/*Second subtracted length in centimetres*/
cm_3_sub:mm_3-10;/*Third subtracted length in centimetres*/
</text>
