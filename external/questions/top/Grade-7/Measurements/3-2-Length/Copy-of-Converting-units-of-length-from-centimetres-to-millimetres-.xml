mm1:(cm1*10);/*First length in millimetres*/
mm2:(cm2*10);/*Second length in millimetres*/
mm3:(cm3*10);/*Third length in millimetres*/

/*Mistake_1*/
mm1_div:cm1/10;/*First divided length in millimetres*/
mm2_div:cm2/10;/*Second divided length in millimetres*/
mm3_div:cm3/10;/*Third divided length in millimetres*/

/*Mistake_2*/
mm1_add:cm1+10;/*First added length in millimetres*/
mm2_add:cm2+10;/*Second added length in millimetres*/
mm3_add:cm3+10;/*Third added length in millimetres*/

/*Mistake_3*/
mm1_sub:cm1-10;/*First added length in millimetres*/
mm2_sub:cm2-10;/*Second added length in millimetres*/
mm3_sub:cm3-10;/*Third added length in millimetres*/
</text>
