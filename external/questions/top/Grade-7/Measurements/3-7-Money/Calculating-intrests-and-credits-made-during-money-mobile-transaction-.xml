    <questionvariables>
      <text>/*Generating random credit limit range*/
credit_range1:rand_with_step(19000,21000,100);

credit_range2:rand_with_step(30000,30700,50);

credit_range3:rand_with_step(30800,31000,100);

credit_range4:rand_with_step(32500,90000,400);

/* Define base interest percentages */
intrest1:rand(16)+1;
intrest2:rand_with_step(17,33,1);
intrest3:rand_with_step(34,50,1);
intrest4:rand_with_step(51,67,1);
intrest5:rand_with_step(68,84,1);
intrest6:rand_with_step(85,100,1);

/* Function to generate random values within a given range */
rand_range(min, max) := min - rand(max - min + 1);

/* Define base credit limit ranges */
base_ranges: [[500, 1000], [1001, 2500], [2501, 5000], [5001, 10000], [10001, 20000], [20001, 30000]];

/* Initialize empty list for randomized ranges */
randomized_ranges: [];

/* Generate randomized ranges using a loop */
for i:1 thru length(base_ranges) do (
    block(
        /* Generate random values within range */
        lower: rand_range(base_ranges[i][1] - 100, base_ranges[i][1] + 100),
        upper: rand_range(base_ranges[i][2] - 100, base_ranges[i][2] + 100),
        
        /* Append range to list */
        randomized_ranges: endcons([lower, upper], randomized_ranges)
    )
);

/* Ensure ranges are sorted and do not overlap */
randomized_ranges: reverse(randomized_ranges); /* Reverse list as endcons appends to the end */

for i:2 thru length(randomized_ranges) do (
    block(
        randomized_ranges[i][1]: max(randomized_ranges[i][1], randomized_ranges[i-1][2] + 1),
        randomized_ranges[i][2]: max(randomized_ranges[i][2], randomized_ranges[i][1] + 500)
    )
);

/* Display output */
randomized_ranges;

Row_1 :randomized_ranges[1];

ranges1:Row_1[1];
ranges2:Row_1[2];

Row_2 :randomized_ranges[2];

ranges3:Row_2[1];
ranges4:Row_2[2];

Row_3:randomized_ranges[3];

ranges5:Row_3[1];
ranges6:Row_3[2];

Row_4 :randomized_ranges[4];

ranges7:Row_4[1];
ranges8:Row_4[2];

Row_5 :randomized_ranges[5];

ranges9:Row_5[1];
ranges10:Row_5[2];

Row_6 :randomized_ranges[6];
