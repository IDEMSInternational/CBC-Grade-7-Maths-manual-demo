      <text>/*Generat random values for kelvin*/
temp_Kelvin1:rand_with_step(273,3000,7);
temp_Kelvin2:rand_with_step(273,3000,7);
temp_Kelvin3:rand_with_step(273,3000,7);
temp_Kelvin4:rand_with_step(273,3000,7);
temp_Kelvin5:rand_with_step(273,3000,7);
temp_Kelvin6:rand_with_step(273,3000,7);
temp_Kelvin7:rand_with_step(273,3000,7);
list_temp_kelvin:([temp_Kelvin1,temp_Kelvin2,temp_Kelvin3,temp_Kelvin4,temp_Kelvin5,temp_Kelvin6,temp_Kelvin7]);
/*Conversion of of temperature of Kelvin to degrees Celsius  */
temp_degrees1:temp_Kelvin1-273;
temp_degrees2:temp_Kelvin2-273;
temp_degrees3:temp_Kelvin3-273;
temp_degrees4:temp_Kelvin4-273;
temp_degrees5:temp_Kelvin5-273;
temp_degrees6:temp_Kelvin6-273;
temp_degrees7:temp_Kelvin7-273;
list_temp_degrees:([temp_degrees1,temp_degrees2,temp_degrees3,temp_degrees4,temp_degrees5,temp_degrees6,temp_degrees7]);
/*Calculating the  average temperature  in degrees */
sum_degrees:temp_degrees1+temp_degrees2+temp_degrees3+temp_degrees4+temp_degrees5+temp_degrees6+temp_degrees7;
average_temp:(temp_degrees1+temp_degrees2+temp_degrees3+temp_degrees4+temp_degrees5+temp_degrees6+temp_degrees7)/7;
average_temp1:decimalplaces(average_temp,1);</text>
