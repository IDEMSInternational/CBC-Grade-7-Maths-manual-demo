/*coordinates of the points for the generated values for the angles on a transverse in degrees */
dtr(d) := d*%pi/180; /* Convert degrees to radians */
angle_to_point(r, d) := [float(r*cos(dtr(d))), float(r*sin(dtr(d)))];
coordinate_pointB: angle_to_point(4,0);
coordinate_pointC: angle_to_point(4,angle_BAC);
/*part (a) answers*/
ta1:t;
ta2:v;
ta3:r;
ta4:s;
ta5:t;
ta6:q;
ta7:w;
ta8:r;

ta9:180-angle_BAC;
